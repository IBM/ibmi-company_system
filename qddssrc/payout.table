create table
    PAYOUT (
        PAYOUTNO int not null generated ALWAYS as identity (
            start
            with
                1,
                increment by 1
        ),
        EMPNO char(6) not null,
        PAY_DATE date not null,
        AMOUNT decimal(9, 2) not null,
        primary key (PAYOUTNO)
    );

